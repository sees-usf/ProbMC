package dipro.run;

/**
 * This class is a root for all models. It contains a reference to the global
 * settings containing the configuration of the search experiment like
 * parameters, algorithm, etc...
 * 
 * @author aljazzar
 * 
 */
public class Model {

	protected final Context settings;

	public Model(Context settings) {
		this.settings = settings;
	}

	public Context getSettings() {
		return settings;
	}
}
